<%= render 'common/sign_in_nav' %>

<div class="test">
  <h1>Login to Bizrev</h1><span id="vert"></span>
  <%= form_for :session, url: login_path do |f| %>
    <h4><%= f.label :username %>:</h4> <%= f.text_field :name %><br>
    <h4><%= f.label :password %>:</h4> <%= f.password_field :password %><br><br>
    <%= f.submit "Login" %>
  <% end %>
</div>
